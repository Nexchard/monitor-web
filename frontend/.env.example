# API 配置
VITE_API_URL=http://localhost:3000/api  # 后端 API 地址

# 应用基础配置
VITE_APP_TITLE=云资源管理系统          # 应用标题
VITE_APP_DEFAULT_WARNING_DAYS=30       # 默认资源到期预警天数
VITE_APP_PAGE_SIZE=10                 # 每页显示数据条数

